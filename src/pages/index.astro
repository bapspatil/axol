          <div
            class="self-stretch flex flex-row items-start justify-center pt-0 pb-0 pr-5 pl-[21px] box-border max-w-full text-left text-brand-colors-node-indigo"
          >
            <div
              id="navbar"
              class="overflow-hidden flex flex-col items-center justify-center max-w-full z-[2]"
            >
              <div
                class="rounded-t-77xl rounded-b-none bg-brand-colors-axol-coral shadow-[0px_0px_3px_rgba(230,_132,_118,_0.5)] overflow-hidden flex flex-row items-center justify-center py-2 px-6 box-border gap-[8px] whitespace-nowrap max-w-full"
